<!DOCTYPE html>
<html>
<head>
  <title>Tunr</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user %>
	<%= current_user.name %>
	<%= link_to 'logout', logout_path, method: :delete %><br>
<% end %>

<%= yield %>

</body>
</html>
